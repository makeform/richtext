.variant { display: none }
.has-variant .variant { display: inline }

.ql-snow svg
  vertical-align: initial
.ql-container
  font-size: 1em
  word-break: break-all
  em
    font-style: italic
    font-weight: inherit
  strong
    font-weight: bold
  img
    max-width: 100%
.ql-tool
  position: sticky
  top: 0
  background: #fff
  z-index: 1
.ql-editor
  min-height: 10em
